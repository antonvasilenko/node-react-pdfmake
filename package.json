{
  "name": "node-react-pdfmake",
  "version": "0.2.4",
  "description": "pdfmake iframe wrapper for react on node and express",
  "main": "dist/",
  "scripts": {
    "build": "npm install && npm run lint && npm run build:clean && npm run build:components",
    "build:clean": "rimraf dist && mkdir dist",
    "build:components": "babel components --out-dir dist -D",
    "lint": "eslint --color \"components/**\""
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0"
  },
  "devDependencies": {},
  "files": [
    "index.js",
    "assets/",
    "node/",
    "dist/"
  ],
  "readmeFilename": "README.md"
}
